module

let pi = 3.1415926535897938

let pow = function b e
  let ret = 1
  loop i e
    ret = ret * b
  end
  return ret
end

let isPrime = function n
  loop i n/2+2
    if n % (i+2) == 0
      return false
    end
  end
  return true
end

let factorial = function n
  if n <= 1
    return 1
  else
    return n * factorial! n - 1 
  end
end